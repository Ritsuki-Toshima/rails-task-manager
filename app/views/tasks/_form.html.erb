<%= form_with model: task do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
    <%= f.label :details %>
    <%= f.text_field :details, class: "form-control" %>
    <%= f.check_box :completed, class: "m-2" %>
    <%= f.label :completed %><br>
    <%= f.submit class: "btn btn-primary mt-2 mb-2"%>
  <% end %>
